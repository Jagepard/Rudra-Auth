{
    "name"              : "rudra/auth",
    "description"       : "Rudra framework",
    "type"              : "library",
    "keywords"          : [
        "rudra",
        "auth"
    ],
    "license"           : "MIT",
    "authors"           : [
        {
            "name"    : "Jagepard",
            "email"   : "jagepard@yandex.ru",
            "homepage": "https://jagepard.ru",
            "role"    : "Developer"
        }
    ],
    "support"           : {
        "email" : "jagepard@yandex.ru"
    },
    "require"           : {
        "php"             : ">=8.0",
        "rudra/container" : "self.version",
        "rudra/redirect"  : "self.version",
        "rudra/docs"      : "self.version"
    },
    "autoload"          : {
        "psr-4" : {
            "Rudra\\Auth\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4": {
            "Rudra\\Auth\\Tests\\" : "tests/"
        }
    },
    "minimum-stability" : "dev",
    "config"            : {
        "optimize-autoloader" : true,
        "prefer-stable"       : true
    }
}
